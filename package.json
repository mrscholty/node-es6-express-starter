{
  "name": "node-es6-express-starter",
  "version": "1.0.0",
  "description": "node.js es6 express boilerplate ",
  "scripts": {
    "test": "mocha --compilers js:babel-core/register test/**/*.js",
    "build": "babel src -d dist && npm run assets",
    "babel-hot": "babel-node -- src/app.js",
    "start": "nodemon --exec \"npm run babel-hot\"",
    "assets": "cp -R src/data dist/data"
  },
  "keywords": [
    "node",
    "es6",
    "express",
    "api",
    "babel",
    "boilerplate"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-2": "^6.24.1",
    "body-parser": "^1.18.2",
    "chai": "^4.0.2",
    "express": "^4.16.2",
    "mocha": "^3.4.2",
    "nodemon": "^1.11.0",
    "supertest": "^3.0.0"
  },
  "dependencies": {}
}
